{
  "expo": {
    "name": "Shelton Springs",
    "slug": "shelton-springs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "sheltonsprings",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rubenjim.sheltonsprings",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.rubenjim.sheltonsprings",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.jpg"
    },
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/"
    },
    "extra": {
      "convexUrl": "${EXPO_PUBLIC_CONVEX_URL}",
      "router": {
        "origin": "https://sheltonsprings.homes"
      },
      "eas": {
        "projectId": "1e1b6e75-8ac2-4fe8-8cff-46c325df0d32"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://sheltonsprings.homes"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-web-browser",
      "expo-font",
      "expo-video"
    ]
  }
}
